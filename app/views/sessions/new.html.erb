<header class="page-header">
<h1>Log In</h1>
</header>

<%= form_for(:session, url: login_path ) do |form| %>
  <div class="form-group">
    <%= form.label :email %> <br>
    <%= form.text_field :email %> 
  </div>

  <div class="form-group">
    <%= form.label :password %> <br>
    <%= form.password_field :password %>
  </div>

    <br>
  <%= form.submit "Log In" %>

<% end %>